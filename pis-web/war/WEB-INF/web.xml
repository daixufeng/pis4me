<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
   	
	<!--sm serlvet-->
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.pis.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>usercreate</servlet-name>
		<servlet-class>com.pis.servlet.UserCreateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>usercreate</servlet-name>
		<url-pattern>/base/usercreate.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>userupdate</servlet-name>
		<servlet-class>com.pis.servlet.UserUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userupdate</servlet-name>
		<url-pattern>/base/userupdate.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>userdelete</servlet-name>
		<servlet-class>com.pis.servlet.UserDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userdelete</servlet-name>
		<url-pattern>/base/userdelete.do</url-pattern>
	</servlet-mapping>
	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
